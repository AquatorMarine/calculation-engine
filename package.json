{
  "name": "@aquatormarine/calculation-engine",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./contract": {
      "types": "./dist/esm/contract/index.d.ts",
      "import": "./dist/esm/contract/index.js",
      "require": "./dist/cjs/contract/index.cjs"
    },
    "./contract/constant": {
      "types": "./dist/esm/contract/constant.d.ts",
      "import": "./dist/esm/contract/constant.js",
      "require": "./dist/cjs/contract/constant.cjs"
    },
    "./rotation": {
      "types": "./dist/esm/rotation/index.d.ts",
      "import": "./dist/esm/rotation/index.js",
      "require": "./dist/cjs/rotation/index.cjs"
    },
    "./rotation/constant": {
      "types": "./dist/esm/rotation/constant.d.ts",
      "import": "./dist/esm/rotation/constant.js",
      "require": "./dist/cjs/rotation/constant.cjs"
    },
    "./dates": {
      "types": "./dist/esm/dates/index.d.ts",
      "import": "./dist/esm/dates/index.js",
      "require": "./dist/cjs/dates/index.cjs"
    },
    "./leave": {
      "types": "./dist/esm/leave/index.d.ts",
      "import": "./dist/esm/leave/index.js",
      "require": "./dist/cjs/leave/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/rename-cjs.js && node scripts/scripts/fix-cjs-requires.js",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "dependencies": {
    "dayjs": "^1.11.19"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}